{
  "name": "@planetadeleste/vuemc",
  "version": "1.0.10",
  "description": "Models and Collections for Vue 3. Maintained version.",
  "license": "MIT",
  "main": "dist/vue-mc.js",
  "module": "dist/vue-mc.es.js",
  "types": "src/vuemc.d.ts",
  "keywords": [
    "vue",
    "collection",
    "datastore",
    "model",
    "models",
    "store",
    "structures",
    "validation"
