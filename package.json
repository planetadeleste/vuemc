    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^5.0.0",
    "mocha": "^6.2.0",
    "moxios": "^0.4.0",
    "nyc": "^14.1.1",
    "rollup": "^1.19.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^8.0.2",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint ./src/**/*.ts",
    "test": "tsc && nyc mocha",
    "docs": "cd docs && jekyll serve",
    "prepublishOnly": "yarn test && yarn build"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.js",
      "src/**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": false
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
