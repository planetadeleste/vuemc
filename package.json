{
  "name": "@dario-peercode/vue-mc",
  "version": "0.6.2",
  "description": "Models and Collections for Vue.js. Maintained version.",
  "license": "MIT",
  "main": "vue-mc.js",
  "module": "vue-mc.es.js",
  "keywords": [
    "vue",
    "collection",
    "datastore",
    "model",
    "models",
    "store",
    "structures",
    "validation"
